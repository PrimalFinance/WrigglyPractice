pragma solidity >=0.4.22 <0.9.0;

contract Solution {
    /**
     * @param {uint256} a
     * @param {uint256} b
     * @return uint256
     */
    function sumOfTwoIntegers(
        uint256 a,
        uint256 b
    ) public pure returns (uint256) {
        //TODO
        // Create an unsigned variable named "sum" to hold the sum of the two parameters.
        uint256 sum = a + b;
        return sum;
    }
}
